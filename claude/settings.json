{
  "env": {
    "ENABLE_TOOL_SEARCH": "true"
  },
  "permissions": {
    "allow": [],
    "deny": [
      "Read(**/.env*)",
      "Edit(**/.env*)",
      "Write(**/.env*)"
    ],
    "defaultMode": "plan"
  },
  "model": "opusplan",
  "enableAllProjectMcpServers": false,
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "spinnerTipsEnabled": false,
  "alwaysThinkingEnabled": false
}
